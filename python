class Importação:
    def __init__(self, nome_carro, pais_Origem) -> None:
        self.nome_carro = nome_carro
        self.pais_Origem = pais_Origem
        self.ativo = False

def __str__(self):
    return f'{self.nome_carro} | {self.pais_Origem}'

carro_mustang = Importação('Mustang', 'Estados Unidos')
carro_ferrari = Importação('Ferrari','Itália',)


Importação = [carro_mustang]
Importação = [carro_ferrari]

print [carro_ferrari]
print [carro_mustang]
